{
  "name": "prime-console",
  "version": "1.1.7",
  "description": "Logging library for Node.js. Supports custom colors, background and formats.",
  "main": "dist/index.js",
  "bin": {
    "prime": "dist/bin/cli.js"
  },
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsup ./src/index.ts --format cjs,esm --dts",
    "dev": "ts-node-dev ./src/index.ts",
    "cli": "ts-node-dev src/bin/cli.ts",
    "pkg": "ts-node-dev src/test.ts",
    "lint": "tsc",
    "release": "pnpm run lint && pnpm run build"
  },
  "keywords": [
    "logging",
    "logger",
    "console",
    "typescript",
    "nodejs",
    "cli",
    "javascript",
    "format"
  ],
  "author": "malezjaa",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^10.0.1",
    "inquirer": "^9.2.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tsup": "^7.0.0"
  }
}
